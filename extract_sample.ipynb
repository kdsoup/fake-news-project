    "import pandas as pd\n",
    "\n",
    "# helper function to extract sample of large dataset\n",
    "src = 'data/995,000_rows.csv'\n",
    "n = 10\n",
    "\n",
    "dst = src[0:-4] + '_SAMPLE.csv'\n",
    "raw_data = pd.read_csv(src)\n",
    "sample_data = raw_data.sample(n)\n",
    "sample_data.to_csv(dst)\n"
